{
  "name": "fred-analyze",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "2.1.0",
    "serve": "10.0.2",
    "surge": "0.20.1"
  },
  "dependencies": {
    "axios": "0.16.1",
    "materialize-css": "1.0.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "scripts": {
    "start": "set PORT=3005&&react-scripts start",
